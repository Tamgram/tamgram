builtins := symmetric-encryption

process out_enc(m, k, 'n) =
  [ Fr(~n) ]-->
  [ Out(senc(<m, 'n>, k)), 'n := n ]

process A =
  [ Fr(~k), Fr(~n) ]-->[ 'k := k, 'n := n ];
  out_enc("A1", 'k, 'n);
  out_enc("A2", 'k, 'n)

process B =
  [ Fr(~k), Fr(~n) ]-->[ 'k := k, 'n := n ];
  out_enc("B1", 'k, 'n);
  out_enc("B2", 'k, 'n)
